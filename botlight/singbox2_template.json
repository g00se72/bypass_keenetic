{
  "log": {
    "disabled": false,
    "level": "error",
    "output": "/opt/etc/sing-box/singbox.log"
  },
  "inbounds": [
    {
      "type": "tun",
      "interface_name": "tun0",
      "address": [
        "172.19.0.1/30"
      ],
      "mtu": 9000,
      "auto_route": false,
      "strict_route": false,
      "endpoint_independent_nat": true,
      "sniff": false,
      "stack": "gvisor",
      "tag": "tun-in-tun0"
    },
    {
      "type": "mixed",
      "tag": "mixed-in-tun0",
      "listen": "0.0.0.0",
      "listen_port": {{localportvless}}
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "tun0",
      "outbounds": [
        "default"
      ],
      "default": "default",
      "interrupt_exist_connections": false
    },
    {
      "type": "vless",
      "tag": "default",
      "server": "{{address}}",
      "server_port": {{port}},
      "uuid": "{{id}}",
      "flow": "{{flow}}",
      "packet_encoding": "xudp",
      "tls": {
        "enabled": true,
        "server_name": "{{sni}}",
        "utls": {
          "enabled": true,
          "fingerprint": "{{fp}}"
        },
        "reality": {
          "enabled": true,
          "public_key": "{{pbk}}",
          "short_id": "{{sid}}"
        }
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "blocked"
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "tun-in-tun0",
        "outbound": "tun0"
      },
      {
        "inbound": "mixed-in-tun0",
        "outbound": "tun0"
      }
    ],
    "auto_detect_interface": false,
    "final": "blocked"
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "external_ui_download_detour": "direct",
      "access_control_allow_private_network": true
    }
  }
}
